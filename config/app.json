{
  "brand": "AnXin Security",
  "themeColor": "#4CA2FF",
  "defaultPage": "overview",
  "minimizeToTray": true,
  "locale": "zh-CN",
  "ui": {
    "animations": true,
    "window": {
      "minWidth": 600,
      "minHeight": 800
    }
  },
  "engine": {
    "autoStart": true,
    "exeRelativePath": "Engine\\Axon_v2\\Axon_ml.exe",
    "processName": "Axon_ml.exe",
    "args": [],
    "exitTimeoutMs": 1000
  },
  "scanner": {
    "baseUrl": "http://127.0.0.1:8000",
    "timeoutMs": 5000,
    "healthPollIntervalMs": 3000,
    "tuningEnabled": true,
    "maxFileSizeMB": 300,
    "tuning": {
      "fastLatencyMs": 700,
      "slowLatencyMs": 2000,
      "minPoolSize": 8,
      "maxPoolSize": 256,
      "minRatePerSecond": 8,
      "maxRatePerSecond": 256,
      "adjustStep": 8
    },
    "maxTokens": 64,
    "ipc": {
      "enabled": true,
      "prefer": true,
      "host": "127.0.0.1",
      "port": 8765,
      "connectTimeoutMs": 500,
      "timeoutMs": 5000
    }
  },
  "scan": {
    "rulesFile": "config/scan_rules.json",
    "traversalTimeoutMs": 2000,
    "walkerBatchSize": 256,
    "cachePersistIntervalMs": 1000
  }
}