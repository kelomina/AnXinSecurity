{
  "brand": "AnXin Security",
  "themeColor": "#4CA2FF",
  "defaultPage": "overview",
  "minimizeToTray": true,
  "locale": "zh-CN",
  "tray": {
    "exitKeepScannerServicePrompt": true,
    "exitKeepScannerServiceDefault": true
  },
  "ui": {
    "animations": true,
    "window": {
      "minWidth": 800,
      "minHeight": 600
    }
  },
  "engine": {
    "autoStart": true,
    "exeRelativePath": "Engine\\Axon_v2\\Axon_ml.exe",
    "processName": "Axon_ml.exe",
    "args": [],
    "exitTimeoutMs": 1000
  },
  "scanner": {
    "timeoutMs": 5000,
    "healthPollIntervalMs": 3000,
    "tuningEnabled": true,
    "maxFileSizeMB": 300,
    "tuning": {
      "fastLatencyMs": 700,
      "slowLatencyMs": 2000,
      "minPoolSize": 8,
      "maxPoolSize": 256,
      "minRatePerSecond": 8,
      "maxRatePerSecond": 256,
      "adjustStep": 8
    },
    "maxTokens": 64,
    "ipc": {
      "enabled": true,
      "prefer": true,
      "host": "127.0.0.1",
      "port": 8765,
      "connectTimeoutMs": 500,
      "timeoutMs": 5000
    }
  },
  "scan": {
    "rulesFile": "config/scan_rules.json",
    "traversalTimeoutMs": 2000,
    "walkerBatchSize": 256,
    "cachePersistIntervalMs": 1000
  },
  "behaviorUi": {
    "pageSize": 0,
    "autoRefreshMs": 0,
    "processListLimit": 0,
    "resolveProcessName": true
  },
  "etw": {
    "enabled": true,
    "sessionName": "AnXinSecuritySession",
    "userDataMaxBytes": 65536,
    "stopTimeoutMs": 2500,
    "startRetries": 2,
    "retryDelayMs": 150,
    "emitRegistryRawHex": false,
    "logToConsole": true,
    "logParsedToConsole": true,
    "resolveProcessName": true,
    "processNameCacheMax": 2048,
    "processNameCacheTtlMs": 300000,
    "processNameResolveMaxPerSecond": 20,
    "consoleMaxPerSecond": 200,
    "network": {
      "enabled": true,
      "filterPrivateIps": true,
      "skipLoopback": true
    }
  },
  "behaviorAnalyzer": {
    "enabled": true,
    "flushIntervalMs": 100,
    "sqlite": {
      "mode": "file",
      "directory": "%TEMP%",
      "fileName": "anxin_etw_behavior.db"
    },
    "filters": {
      "Registry": {
        "skipOps": [
          "OpenKey",
          "CloseKey",
          "QueryValue",
          "QueryKey",
          "EnumerateKey",
          "SetInformationKey",
          "EnumerateValue"
        ]
      },
      "Process": {
        "skipOps": []
      },
      "File": {
        "skipOps": []
      }
    }
  },
  "behaviorMitre": {
    "enabled": true,
    "tactics": [
      "Initial Access",
      "Execution",
      "Persistence",
      "Privilege Escalation",
      "Defense Evasion",
      "Credential Access",
      "Discovery",
      "Lateral Movement",
      "Collection",
      "Command and Control",
      "Exfiltration",
      "Impact"
    ],
    "rules": [
      {
        "provider": "Process",
        "op": "Start",
        "tactic": "Execution",
        "techniqueId": "T1106",
        "techniqueName": "Native API"
      },
      {
        "provider": "Process",
        "op": "Stop",
        "tactic": "Impact",
        "techniqueId": "T1485",
        "techniqueName": "Data Destruction"
      },
      {
        "provider": "File",
        "op": "Create",
        "tactic": "Collection",
        "techniqueId": "T1005",
        "techniqueName": "Data from Local System"
      },
      {
        "provider": "File",
        "op": "Delete",
        "tactic": "Defense Evasion",
        "techniqueId": "T1070",
        "techniqueName": "Indicator Removal"
      },
      {
        "provider": "File",
        "op": "Rename",
        "tactic": "Defense Evasion",
        "techniqueId": "T1070",
        "techniqueName": "Indicator Removal"
      },
      {
        "provider": "Registry",
        "op": "SetValue",
        "tactic": "Persistence",
        "techniqueId": "T1112",
        "techniqueName": "Modify Registry"
      }
    ]
  }
}